{
    "cli": {
        "version": ">= 16.2.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "simulator": {
            "extends": "development",
            "ios": {
                "simulator": true
            }
        },
        "preview": {
            "distribution": "internal",
            "environment": "preview",
            "autoIncrement": true,
            "channel": "alpha"
        },
        "store": {
            "distribution": "store",
            "environment": "preview",
            "autoIncrement": true,
            "channel": "alpha"
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "store": {},
        "production": {}
    }
}